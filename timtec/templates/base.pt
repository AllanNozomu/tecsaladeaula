<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <title>timtec</title>

    <link rel="stylesheet/less" href="${request.static_url('timtec:static/css/bootstrap/bootstrap.less')}" type="text/css" />
    <link rel="stylesheet/less" href="${request.static_url('timtec:static/fonts/font-awesome/stylesheet.css')}" type="text/css" />

    <script src="${request.static_url('timtec:static/js/less-1.4.1.min.js')}" type="text/javascript"></script>
    <script src="${request.static_url('timtec:static/js/jquery-1.10.2.min.js')}" type="text/javascript"></script>
    <script src="${request.static_url('timtec:static/js/bootstrap.min.js')}" type="text/javascript"></script>

</head>

<body>
    <!-- NAVBAR -->
    <div id="main-navbar" class="navbar navbar-fixed-top navbar-inverse">
        <div class="container">
            <a class="navbar-brand" href="/">TIM Tec</a>
            <ul class="nav navbar-nav pull-right">
                <li><a href="#">Cursos</a></li>
                <li>
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Entrar</a>
                    <div class="dropdown-menu dropdown-signin">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label class="col-sm-3 col-lg-3 control-label">E-mail</label>
                                <div class="col-sm-9 col-lg-9">
                                    <input type="e-mail" class="form-control" placeholder="Qual o seu email?">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 col-lg-3 control-label">Senha</label>
                                <div class="col-sm-9 col-lg-9">
                                    <input type="password" class="form-control" placeholder="Senha">
                                </div>
                            </div>
                            <div class="submit">
                                <div class="row">
                                    <div class="col-sm-8 col-lg-8">
                                        <a href="">Esqueci minha senha</a>
                                    </div>
                                    <div class="col-sm-4 col-lg-4 textright">
                                        <button class="btn btn-success">Entrar</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div class="divider"></div>
                        <p class="bottom textcenter">Entrar com <a href="">Facebook</a> ou <a href="">Google+</a></p>
                    </div>
                </li>
                <li>
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Cadastrar</a>
                    <div class="dropdown-menu dropdown-signup">
                        <form class="form-horizontal">
                            <div class="form-group">
                                <label class="col-sm-4 col-lg-4 control-label">Nome Completo</label>
                                <div class="col-sm-8 col-lg-8">
                                    <div class="col-sm-6 col-lg-6 left">
                                        <input type="text" class="form-control" placeholder="Primeiro nome">
                                    </div>
                                    <div class="col-sm-6 col-lg-6 right">
                                        <input type="text" class="form-control" placeholder="Último nome">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-4 col-lg-4 control-label">E-mail</label>
                                <div class="col-sm-8 col-lg-8">
                                    <input type="e-mail" class="form-control" placeholder="Qual o seu email?">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-4 col-lg-4 control-label">Senha</label>
                                <div class="col-sm-8 col-lg-8">
                                    <div class="col-sm-6 col-lg-6 left">
                                        <input type="password" class="form-control" placeholder="Senha">
                                    </div>
                                    <div class="col-sm-6 col-lg-6 right">
                                        <input type="password" class="form-control" placeholder="Senha novamente">
                                    </div>
                                </div>
                            </div>
                            <div class="submit textright">
                                <button class="btn btn-success">Cadastrar</button>
                            </div>
                        </form>
                        <div class="divider"></div>
                        <p class="bottom textcenter">Cadastrar com <a href="">Facebook</a> ou <a href="">Google+</a></p>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- END NAVBAR -->

    <!-- CONTAINER -->
    <metal:content define-slot="container" />
    <!-- END CONTAINER -->

    <!-- FOOTER -->
    <div class="footer">
        <div class="container">
            <div class="row">
                <div class="col-sm-3 col-lg-3">
                    <ul class="no-bullet">
                        <li><a href="">Sobre</a></li>
                        <li><a href="">Equipe</a></li>
                        <li><a href="">Imprensa</a></li>
                        <li><a href="">Contato</a></li>
                    </ul>
                </div>
                <div class="col-sm-3 col-lg-3">
                    <ul class="no-bullet">
                        <li><a href="">Blog</a></li>
                        <li><a href="">Ajuda</a></li>
                        <li><a href="">FAQ</a></li>
                    </ul>
                </div>
                <div class="col-sm-3 col-lg-3">
                    <ul class="no-bullet">
                        <li><a href="">Política de Uso</a></li>
                        <li><a href="">Política de Privacidade</a></li>
                    </ul>
                </div>
                <div class="col-sm-3 col-lg-3">
                    Coluna 4
                </div>
            </div>
        </div>
    </div>
    <!-- END FOOTER -->

</body>
</html>
